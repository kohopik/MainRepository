@model IEnumerable<IndieProjects.Model.Article>

<li><a href="~/Account/">Обо мне</a></li>
<li><a href="~/Account/Notification">Активность</a></li>
<li><a href="~/Account/MyProjects">Мои проекты</a></li>
<li><a href="~/Account/ProfileChanges">Редактировать профиль</a></li>
<div>
    <a href="~/Account/AddArticle/">Написать статью</a>
    @foreach (var article in Model)
    {
        <div>
            <label>@article.Title</label><br />
            <label id="txt"></label><br />
            @Html.Raw(article.Content)
            <label>@article.Likes</label>
            <a href="~/Home/CurrentArticle/@article.ID">Перейти</a>
        </div><br />
    }
</div>